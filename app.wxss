/**app.wxss**/

view, image, text, input,button {
    box-sizing: border-box;
    padding: 0;
    margin: 0;
}

swiper {
    height: auto;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

/*搜索  */

.container{
  background: #f5f5f5;
  min-height: 1206rpx;
}
.head-view{
  display: flex;
}

.search-content {
    background: #f5f5f5;
    width: 580rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    padding-top: auto;
    padding-left: 20rpx;
    padding-bottom: auto;
    margin-left: 20rpx;
    height: 70rpx;
    border-radius: 100px;
}

.search-icon {
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}

.search-input {
    width: 500rpx;
    font-size: 28rpx;
}

.red {
    color: #FF3434!important;
}

.fl{
  float: left;
}

.fr{
  float: right;
}

.ib{
  display: inline-block;
}

.hide{
  display: none;
}

.noscroll {
  height: 100%;
  overflow: hidden;
}

.mask {
    background:rgba(0,0,0,.5);
    position: fixed;
    top:0;
    left:0;
    width:100%;
    height:100%;
    z-index:10;
}

.mask-dialog {
    position: absolute;
    top:50%;
    left:50%;
    width:630rpx;
    transform:translate(-50%,-50%);
}

.clearfix::after{
    content: "";
    height: 0;
    line-height: 0;
    display: block;
    clear: both;
}

.c9 {
    color: #999!important;
    font-size: 12px;
}
.ib{
  display: inline-block;
}
.c3{
  color: #333
}